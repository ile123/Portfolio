package com.petstore.common.DTO;

public class BrandDTO {
	private Integer Id;
	private String name;
	public Integer getId() {
		return Id;
	}
	public void setId(Integer id) {
		Id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public BrandDTO(Integer id, String name) {
		Id = id;
		this.name = name;
	}
	
	
}
